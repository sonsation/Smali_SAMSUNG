<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/sem_resolver_bg_radius" />
            <solid android:color="@color/sem_resolver_bg_color" />
        </shape>
    </item>
    <item android:gravity="bottom">
        <shape android:shape="rectangle">
            <solid android:color="@color/sem_resolver_bg_color" />
            <size android:height="@dimen/sem_resolver_bg_radius" />
        </shape>
    </item>
</layer-list>