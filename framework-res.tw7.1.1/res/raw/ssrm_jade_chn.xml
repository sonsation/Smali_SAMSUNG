<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE siop_document [
<!ELEMENT siop_document (levels , default_scenario , extra_scenario?)>
<!ELEMENT levels (level)+>
<!ELEMENT level EMPTY>
<!ATTLIST level
          id CDATA #REQUIRED
          temp CDATA #REQUIRED>
<!ELEMENT default_scenario (limiter)+>
<!ELEMENT limiter (sts_item* , table_item*)>
<!ATTLIST limiter
          name CDATA #REQUIRED
          type (STS|number|bool) #REQUIRED>
<!ELEMENT sts_item EMPTY>
<!ATTLIST sts_item
          pre CDATA #IMPLIED
          perc CDATA #IMPLIED
          trigger CDATA #REQUIRED
          release CDATA #REQUIRED
          min CDATA #IMPLIED
          max CDATA #IMPLIED
          duration CDATA #IMPLIED>
<!ELEMENT table_item EMPTY>
<!ATTLIST table_item
          temp CDATA #REQUIRED
          value CDATA #REQUIRED
          low_temp CDATA #IMPLIED>
<!ELEMENT extra_scenario (scenario)+>
<!ELEMENT scenario (trigger_statuses? , trigger_packages? , limiter*)>
<!ATTLIST scenario name CDATA #REQUIRED>
<!ATTLIST scenario priority CDATA #IMPLIED>
<!ELEMENT trigger_statuses (status)+>
<!ELEMENT status EMPTY>
<!ATTLIST status name CDATA #REQUIRED>
<!ELEMENT trigger_packages (package)+>
<!ELEMENT package EMPTY>
<!ATTLIST package name CDATA #REQUIRED>
] >

<siop_document>

    <model_settings>
        <setting name="FunctionsLimitation">
            <set ovht_pst="480" ovht_bat="430" />
        </setting>
    </model_settings>

    <levels>
        <level id="-3" temp="320" />
        <level id="-2" temp="340" />
        <level id="-1" temp="360" />
        <level id="0" temp="380" />
        <level id="1" temp="400" />
        <level id="2" temp="420" />
        <level id="3" temp="440" />
        <level id="4" temp="460" />
        <level id="5" temp="480" />
        <level id="6" temp="500" />
        <level id="7" temp="520" />
        <level id="8" temp="540" />
    </levels>

    <default_scenario>
        <limiter name="CPUFreqMax" type="STS">
            <sts_item trigger="390" release="350" min="1261000" />
        </limiter>
        <limiter name="GPUFreqMax" type="STS">
            <sts_item trigger="390" release="350" min="485000" />
        </limiter>
        <limiter name="PST" type="number">
            <table_item temp="380" value="1" />
        </limiter>
        <limiter name="Charging" type="number">
            <table_item temp="420" value="70" />
            <table_item temp="440" value="10" />
            <table_item temp="480" value="10" />
            <table_item temp="500" value="0" />
        </limiter>
        <limiter name="LCDBrightness" type="number">
            <table_item temp="420" value="-1" />
            <table_item temp="440" value="230" />
            <table_item temp="460" value="207" />
        </limiter>
        <limiter name="MaxVolume" type="number">
            <table_item temp="380" value="-1" />
        </limiter>
        <limiter name="AppInstallStandby" type="number">
            <table_item temp="340" value="-1" />
            <table_item temp="360" value="3000" />
            <table_item temp="380" value="5000" />
        </limiter>
        <limiter name="SmartBonding" type="bool">
            <table_item temp="440" value="0" />
        </limiter>
    </default_scenario>

    <extra_scenario>

        <scenario name="ScreenOff" priority="1">
            <limiter name="Charging" type="number">
                <table_item temp="380" value="100" />
            </limiter>
            <limiter name="AppInstallStandby" type="number">
                <table_item temp="340" value="5000" />
                <table_item temp="380" value="10000" />
            </limiter>
        </scenario>

        <scenario name="ScreenOffInCall" priority="1">
            <limiter name="Charging" type="number">
                <table_item temp="380" value="10" />
            </limiter>
        </scenario>

        <scenario name="BaiduStore">
            <trigger_packages>
                <package name="com.baidu.appsearch" />
                <package name="com.samsung.android.packageinstaller" />
            </trigger_packages>

            <limiter name="Charging" type="number">
                <table_item temp="350" value="10" />
                <table_item temp="400" value="0" />
            </limiter>
        </scenario>

        <scenario name="Navigation">
            <trigger_packages>
                <package name="com.google.android.apps.maps" />
                <package name="com.skt.skaf.l001mtm091" />
                <package name="kt.navi" />
                <package name="com.mnsoft.lgunavi" />
                <package name="com.locnall.KimGiSa" />
                <package name="kr.mappers.AtlanSmart" />
                <package name="com.mnsoft.mappyobn" />
                <package name="com.thinkware.inaviair" />
                <package name="com.thinkware.sundo.inavi3d" />
                <package name="net.daum.android.map" />
                <package name="com.nhn.android.nmap" />
                <package name="com.autonavi.minimap" />
                <package name="com.sogou.map.android.maps" />
                <package name="com.tencent.map" />
                <package name="com.baidu.BaiduMap" />
            </trigger_packages>

            <limiter name="Charging" type="number">
                <table_item temp="380" value="10" />
                <table_item temp="400" value="0" />
            </limiter>
        </scenario>

        <scenario name="BrowserScenario">
            <limiter name="Charging" type="number">
                <table_item temp="350" value="10" />
                <table_item temp="370" value="0" />
            </limiter>
