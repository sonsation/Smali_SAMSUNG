        <scenario name="CameraBokehShot">
            <trigger_statuses>
                <status name="Camera_bokehShot" />
            </trigger_statuses>

            <limiter name="CPUFreqMax" type="number">
                <table_item temp="380" value="-1" />
            </limiter>
            <limiter name="GPUFreqMax" type="number">
                <table_item temp="380" value="-1" />
            </limiter>
        </scenario>

        <scenario name="GameThermalCpuControl-4">
