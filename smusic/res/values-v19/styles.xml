<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="NotificationBigText" parent="@android:style/TextAppearance.StatusBar.EventContent">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">7</item>
        <item name="android:fontFamily">sec-roboto-light</item>
    </style>
    <style name="NotificationText" parent="@android:style/TextAppearance.StatusBar.EventContent">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sec-roboto-light</item>
    </style>
    <style name="NotificationTitle" parent="@android:style/TextAppearance.StatusBar.EventContent.Title">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sec-roboto-light</item>
    </style>
</resources>
