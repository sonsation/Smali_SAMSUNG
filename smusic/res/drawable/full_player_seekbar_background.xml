<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/full_player_seekbar_background_theme" />
            <stroke android:width="@dimen/full_player_seekbar_stroke_size" android:color="@color/full_player_seekbar_stroke_theme" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/full_player_seekbar_secondary_progress_theme" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/full_player_seekbar_progress_theme" android:endColor="@color/full_player_seekbar_progress_theme" />
            </shape>
        </clip>
    </item>
</layer-list>