    </FrameLayout>
    <FrameLayout android:layout_gravity="end" android:id="@id/volume_button" android:focusable="true" android:layout_width="@dimen/full_player_favorite_volume_width" android:layout_height="@dimen/full_player_favorite_volume_height">
