<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout android:layout_gravity="start" android:id="@id/list_button" android:focusable="true" android:layout_width="@dimen/full_player_favorite_volume_width" android:layout_height="@dimen/full_player_favorite_volume_height">
        <ImageView android:layout_gravity="center" android:id="@id/list_icon" android:background="@drawable/unbounded_ripple_light_full_player_button" android:duplicateParentState="true" android:layout_width="@dimen/full_player_button_image_size" android:layout_height="@dimen/full_player_button_image_size" android:src="@drawable/music_player_ic_queue" android:scaleType="fitXY" android:contentDescription="@string/queue" />
    </FrameLayout>
    <FrameLayout android:layout_gravity="center" android:id="@id/favorite_button" android:focusable="true" android:layout_width="@dimen/full_player_favorite_volume_width" android:layout_height="@dimen/full_player_favorite_volume_height">
